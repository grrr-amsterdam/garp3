{
	"listFields": ["identifier"],
	"inputs": {
		"identifier": {
			"maxLength": 30,
			"unique": true
		},
		"uri": {
			"maxLength": 255
		},
		"name": {
			"required": false
		},
		"html": {
			"type": "html",
			"required": false
		},
		"text": {
			"required": false
		},
		"has_name": {
			"type": "checkbox",
			"default": 0
		},
		"has_html": {
			"type": "checkbox",
			"default": 0
		},
		"has_text": {
			"type": "checkbox",
			"default": 0
		},
		"has_image": {
			"type": "checkbox",
			"default": 0
		}
	},
	
	"relations": {
		"Image": {
			"type": "hasOne"
		}
	},

	"behaviors": {
		"Sluggable": {
			"baseField": "identifier"
		}
	},
		
	"module": "garp"
}
